<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context="com.example.administrator.wo.wo_make"
    android:orientation="vertical">
    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#ffffff">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:gravity="bottom"
                android:text="设置"
                android:textColor="#0da17f"
                android:layout_marginLeft="10dp"
                android:textSize="20dp"
                android:id="@+id/textView16" />
        </TableRow>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#dddddd">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                />
        </TableRow>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#ffffff"
            android:id="@+id/make_xiaoxi">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:text="消息提醒"
                android:gravity="center"
                android:textColor="#000000"
                android:layout_marginLeft="10dp"
                android:textSize="20dp"
                android:id="@+id/textView17" />
            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center"
                android:layout_marginLeft="110dp"
                android:src="@drawable/android_list_idex"
                android:id="@+id/imageView6" />

        </TableRow>
        <View style="@style/PersonalLine"
            android:background="#eeeeee"
            android:layout_height="3dp"/>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/t_make_ziti"
            android:background="#ffffff">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:text="字号大小"
                android:textColor="#000000"
                android:gravity="center"
                android:layout_marginLeft="10dp"
                android:textSize="20dp"
                android:id="@+id/textView19" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:text="中号字(默认)"
                android:textColor="#000000"
                android:gravity="center"
                android:layout_marginLeft="110dp"
                android:textSize="19dp"
                android:id="@+id/textView25" />
        </TableRow>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#dddddd">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                />
        </TableRow>
        <View style="@style/PersonalLine"
            android:background="#eeeeee"
            android:layout_height="3dp"/>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#ffffff">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:text="清除缓存"
                android:textColor="#000000"
                android:gravity="center"
                android:layout_marginLeft="10dp"
                android:textSize="20dp"
                android:id="@+id/textView20" />
            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center"
                android:layout_marginLeft="110dp"
                android:src="@drawable/android_list_idex"
                android:id="@+id/imageView" />
        </TableRow>
        <View style="@style/PersonalLine"
            android:background="#eeeeee"
            android:layout_height="3dp"/>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#ffffff">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="其他"
                android:textColor="#000000"
                android:layout_marginLeft="10dp"
                android:textSize="20dp"
                android:id="@+id/textView21" />
            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center"
                android:layout_marginLeft="110dp"
                android:src="@drawable/android_list_idex"
                android:id="@+id/imageView1" />
        </TableRow>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#dddddd">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                />
        </TableRow>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#ffffff">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:text="扫一扫"
                android:textColor="#000000"
                android:gravity="center"
                android:layout_marginLeft="10dp"
                android:textSize="20dp"
                android:id="@+id/textView22" />
            <ImageView
                android:layout_width="20dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:layout_marginLeft="110dp"
                android:src="@drawable/sao"
                android:id="@+id/imageView2" />
        </TableRow>

        <View style="@style/PersonalLine"
            android:background="#eeeeee"
            android:layout_height="3dp"/>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#ffffff">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:text="关于我们"
                android:textColor="#000000"
                android:gravity="center"
                android:layout_marginLeft="10dp"
                android:textSize="20dp"
                android:id="@+id/textView23" />
            <ImageView
                android:layout_width="20dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:layout_marginLeft="110dp"
                android:src="@drawable/about_we"
                android:id="@+id/imageView3" />
        </TableRow>
        <View style="@style/PersonalLine"
            android:background="#eeeeee"
            android:layout_height="3dp"/>
        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#ffffff">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:text="检查更新"
                android:textColor="#000000"
                android:gravity="center"
                android:layout_marginLeft="10dp"
                android:textSize="20dp"
                android:id="@+id/textView24" />
            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center"
                android:layout_marginLeft="110dp"
                android:src="@drawable/android_list_idex"
                android:id="@+id/imageView4" />
        </TableRow>
        <View style="@style/PersonalLine"
            android:background="#eeeeee"
            android:layout_height="3dp"/>
    </TableLayout>



</LinearLayout>
